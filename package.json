{
  "name": "quadstore",
  "version": "10.0.0-beta.0",
  "description": "A LevelDB-backed graph database for Node.js and the browser. Supports quads, RDF/JS interfaces and SPARQL queries.",
  "keywords": [
    "node",
    "nodejs",
    "quadstore",
    "graph",
    "rdf",
    "database",
    "sparql",
    "rdfjs",
    "browser"
  ],
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "mocha ./test",
    "ts:build": "tsc --project ./tsconfig.json",
    "ts:watch": "mkdir -p dist && tsc --watch --project ./tsconfig.json",
    "build": "npm run ts:build",
    "publish:alpha": "npm publish --tag alpha",
    "publish:beta": "npm publish --tag beta"
  },
  "files": [
    "dist/lib/**/*.js",
    "dist/lib/**/*.d.ts",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/belayeng/quadstore.git"
  },
  "devDependencies": {
    "@types/abstract-leveldown": "^7.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/leveldown": "^4.0.3",
    "@types/memdown": "^3.0.0",
    "@types/n3": "^1.10.4",
    "@types/rdf-js": "^4.0.1",
    "@types/rocksdb": "^3.0.1",
    "abstract-leveldown": "^7.2.0",
    "fs-extra": "^10.0.0",
    "leveldown": "^6.1.0",
    "memdown": "^6.1.1",
    "mocha": "^9.2.0",
    "n3": "^1.13.0",
    "rdf-data-factory": "^1.1.0",
    "rocksdb": "^5.2.0",
    "should": "^13.2.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "asynciterator": "^3.2.1",
    "js-sorted-set": "^0.7.0",
    "nanoid": "^3.3.1"
  },
  "author": "Jacopo Scazzosi <jacopo@scazzosi.com>",
  "contributors": [
    "Matteo Murgida <teomurgi@gmail.com>"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/belayeng/quadstore",
  "bugs": "https://github.com/belayeng/quadstore/issues"
}
