{
  "name": "quadstore",
  "version": "11.0.0-beta.3",
  "description": "A LevelDB-backed graph database for Node.js and the browser. Supports quads, RDF/JS interfaces and SPARQL queries.",
  "keywords": [
    "node",
    "nodejs",
    "quadstore",
    "graph",
    "rdf",
    "database",
    "sparql",
    "rdfjs",
    "browser"
  ],
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "test": "npm run clean && npm run build:cjs && mocha ./test",
    "dev": "npm run clean && tsc -p tsconfig.cjs.json --watch",
    "clean": "rm -rf dist",
    "build:esm-fix-import-paths": "node fix-esm-import-paths.js",
    "build:esm": "tsc -p tsconfig.esm.json && echo '{\"type\": \"module\"}' > dist/esm/package.json && npm run build:esm-fix-import-paths",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "publish:alpha": "npm run build && npm publish --tag alpha",
    "publish:beta": "npm run build && npm publish --tag beta"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/belayeng/quadstore.git"
  },
  "devDependencies": {
    "@types/n3": "^1.10.4",
    "@types/rdf-js": "^4.0.1",
    "abstract-level": "^1.0.3",
    "classic-level": "^1.2.0",
    "memory-level": "^1.0.0",
    "mocha": "^10.0.0",
    "n3": "^1.16.2",
    "rdf-data-factory": "^1.1.1",
    "should": "^13.2.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "asynciterator": "^3.7.0",
    "js-sorted-set": "^0.7.0"
  },
  "author": "Jacopo Scazzosi <jacopo@scazzosi.com>",
  "contributors": [
    "Matteo Murgida <teomurgi@gmail.com>"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/belayeng/quadstore",
  "bugs": "https://github.com/belayeng/quadstore/issues"
}
